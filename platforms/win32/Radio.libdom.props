<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
    <Import Project="$(SolutionDir)\libdom-inc.props" Condition="exists('$(SolutionDir)\libnet-inc.props')" />
  </ImportGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>Expat DOM Library</_PropertySheetDisplayName>
    <IncludePath>$(SolutionDir)..\..\libdom\expat\inc;$(IncludePath)</IncludePath>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" />
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_LIB;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <SubSystem>Windows</SubSystem>
      <GenerateDebugInformation>true</GenerateDebugInformation>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>